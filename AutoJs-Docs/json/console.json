        {
          "textRaw": "console.assert(value, message)",
          "type": "method",
          "name": "assert",
          "signatures": [
            {
              "params": [
                {
                  "textRaw": "value {any} 要断言的布尔值 ",
                  "name": "value",
                  "type": "any",
                  "desc": "要断言的布尔值"
                },
                {
                  "textRaw": "message {string} value为false时要输出的信息 ",
